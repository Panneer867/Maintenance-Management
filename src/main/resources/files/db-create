ALTER SESSION SET "_ORACLE_SCRIPT"=TRUE;

DROP USER MAINTENANCE_MANAGEMENT CASCADE;

CREATE USER MAINTENANCE_MANAGEMENT IDENTIFIED BY MMDB;

GRANT CONNECT TO MAINTENANCE_MANAGEMENT;
GRANT CREATE SESSION TO MAINTENANCE_MANAGEMENT;
GRANT RESOURCE, DBA TO MAINTENANCE_MANAGEMENT;
GRANT UNLIMITED TABLESPACE TO MAINTENANCE_MANAGEMENT;
GRANT ALL PRIVILEGES TO MAINTENANCE_MANAGEMENT;
GRANT IMP_FULL_DATABASE TO MAINTENANCE_MANAGEMENT;